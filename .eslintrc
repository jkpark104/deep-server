{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off"
  }
}
